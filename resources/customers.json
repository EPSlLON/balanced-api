{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
        "id": {
            "type": "string",
            "format": "uri",
            "required": true
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "required": true
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "required": true
        },
        "name": {
            "type": ["string", "null"],
            "required": true
        },
        "business_name": {
            "type": ["string", "null"],
            "required": true
        },
        "email": {
            "type": ["string", "null"],
            "format": "email",
            "required": true
        },
        "source": {
            "anyOf": [
                { "type": "null" },
                { "$ref": "cards.json" },
                { "$ref": "bank_accounts.json" }
            ],
            "required": true
        },
        "destination": {
            "anyOf": [
                { "type": "null" },
                { "$ref": "bank_accounts.json" }
            ],
            "required": true
        },
        "phone": {
            "type": ["string", "null"],
            "format": "phone",
            "required": true
        },
        "ssn_last4": {
            "type": ["string", "null"],
            "pattern": "[0-9]{4}",
            "required": true
        },
        "ein": {
            "type": "string",
            "pattern": "[0-9]{9}",
            "required": true
        },
        "dob_month": {
            "type": "integer",
            "minimum": 1,
            "maximum": 12,
            "required": true
        },
        "dob_year": {
            "type": "integer",
            "required": true
        },
        "merchant_status": {
            "type": "string",
            "enum": ["need-more-information", "underwritten", "rejected"]
        },
        "cards_uri": {
            "type": "string",
            "format": "uri",
            "required": true
        },
        "bank_accounts_uri": {
            "type": "string",
            "format": "uri",
            "required": true
        },
        "debits_uri": {
            "type": "string",
            "format": "uri",
            "required": true
        },
        "credits_uri": {
            "type": "string",
            "format": "uri",
            "required": true
        },
		"address": {
            "$ref": "../address.json",
            "required": true
		},
        "meta": {
            "type": "object",
            "required": true
        }
	},
    "additionalProperties": false
}
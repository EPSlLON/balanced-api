{
	"$schema": "http://json-schema.org/draft-03/schema#",
	"type": "object",
	"properties": {
        "uri": {
            "type": "string",
            "format": "uri"
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        },
        "name": {
            "type": "string"
        },
        "business_name": {
            "type": "string"
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "source": {
            "$ref": "<url to financial instrument"
        },
        "destination": {
            "$ref": "<url to financial instrument"
        },
        "phone": {
            "type": "string",
            "format": "phone"
        },
        "ssn_last4": {
            "type": "string",
            "minLength": 4,
            "maxLength": 4
        },
        "ein": {
            "type": "string",
            "minLength": 9,
            "maxLength": 9
        },
        "dob_month": {
            "type": "integer",
            "minimum": 1,
            "maximum": 12
        },
        "dob_year": {
            "type": "integer"
        },
        "cards_uri": {
            "type": "string",
            "format": "uri"
        },
        "bank_accounts_uri": {
            "type": "string",
            "format": "uri"
        },
        "debits_uri": {
            "type": "string",
            "format": "uri"
        },
        "credits_uri": {
            "type": "string",
            "format": "uri"
        },
		"address": {
            "$ref": "<url to address definition"
		},
        "meta": {
            "type": "object"
        }
	}
}
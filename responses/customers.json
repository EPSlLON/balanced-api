{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
	"links": {
	    "type": "object",
	    "properties": {
		"customers.cards": { "type": "string", "format": "uri", "required": true },
		"customers.bank_accounts": { "type": "string", "format": "uri", "required": true },
		"customers.credits": { "type": "string", "format": "uri", "required": true },
		"customers.debits": { "type": "string", "format": "uri", "required": true },
		"customers.refunds": { "type": "string", "format": "uri", "required": true },
		"customers.reversals": { "type": "string", "format": "uri", "required": true },
		"customers.transactions": { "type": "string", "format": "uri", "required": true }
	    }
	    "required": true
	},
	"meta": {
	    "type": "object",
	    "required": false
	},
	"customers": { "items": { "$ref": "models/customers.json" } "type": "array", "minItems": 1, "uniqueItems": true },
    },
}

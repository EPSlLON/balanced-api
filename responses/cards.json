{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "links": {
            "type": "object",
            "properties": {
                "cards.holds": {
                    "type": "string",
                    "format": "uri",
                    "required": true
                },
                "cards.credits": {
                    "type": "string",
                    "format": "uri",
                    "required": true
                },
                "cards.debits": {
                    "type": "string",
                    "format": "uri",
                    "required": true
                }
            },
            "required": false
        },
        "meta": {
            "type": "object",
            "required": false
        },
        "cards": {
            "items": {
                "$ref": "models/cards.json"
            },
            "type": "array",
            "minItems": 1,
            "uniqueItems": true
        }
    }
}
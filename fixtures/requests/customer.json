{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "/customers",
    "type": "object",
    "properties": {
        "name": {
            "type": [
                "string",
                "null"
            ]
        },
        "business_name": {
            "type": [
                "string",
                "null"
            ]
        },
        "email": {
            "type": [
                "string",
                "null"
            ],
            "format": "email"
        },
        "phone": {
            "type": [
                "string",
                "null"
            ],
            "format": "phone"
        },
        "ssn_last4": {
            "type": [
                "string",
                "null"
            ],
            "pattern": "[0-9]{4}"
        },
        "ein": {
            "type": [
                "string",
                "null"
            ],
            "pattern": "[0-9]{9}"
        },
        "dob_month": {
            "anyOf": [
                {
                    "$ref": "_models/month.json"
                },
                {
                    "type": "null"
                }
            ]
        },
        "dob_year": {
            "anyOf": [
                {
                    "$ref": "_models/year.json"
                },
                {
                    "type": "null"
                }
            ]
        },
        "address": {
            "$ref": "_models/address.json"
        },
        "meta": {
            "type": "object"
        }
    }
}